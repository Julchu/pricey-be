{
  "name": "pricey-be",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch --env-file=.env.development src/index.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint",
    "db-generate": "drizzle-kit generate",
    "dev-env": "DATABASE_URL=$(grep DATABASE_URL -w .env.development | cut -d '=' -f2- | cut -d '\"' -f2 )",
    "dev-env-supabase": "pnpm run dev-env SUPABASE_CRT=$(grep SUPABASE_CRT -w .env.development | cut -d '=' -f2- | cut -d '\"' -f2 )",
    "db-migrate-dev": "pnpm run dev-env drizzle-kit migrate",
    "db-push-dev": "pnpm run dev-env drizzle-kit push",
    "db-studio-dev": "pnpm run dev-env drizzle-kit studio --port=3002",
    "clean-compiled-js": "tsc --build --clean",
    "build": "pkgroll"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "drizzle-orm": "^0.41.0",
    "drizzle-seed": "^0.3.1",
    "express": "5.0.0",
    "express-rate-limit": "^7.5.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "postgres": "^3.4.5"
  },
  "packageManager": "pnpm@10.6.3+sha512.bb45e34d50a9a76e858a95837301bfb6bd6d35aea2c5d52094fa497a467c43f5c440103ce2511e9e0a2f89c3d6071baac3358fc68ac6fb75e2ceb3d2736065e6",
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.1",
    "@types/express-serve-static-core": "^5.0.6",
    "@types/http-errors": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.0",
    "drizzle-kit": "^0.30.6",
    "eslint": "^9.24.0",
    "globals": "^16.0.0",
    "pkgroll": "^2.12.1",
    "prettier": "^3.5.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0"
  },
  "engines": {
    "node": ">=23.11.0"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  }
}